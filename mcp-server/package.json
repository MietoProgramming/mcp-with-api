{
    "name": "mcp-api-server",
    "version": "1.0.0",
    "description": "MCP server for interacting with the NestJS API",
    "type": "module",
    "main": "dist/index.js",
    "bin": {
        "mcp-api-server": "./dist/index.js"
    },
    "scripts": {
