{
    "name": "mcp-api-server-nestjs",
    "version": "1.0.0",
    "description": "NestJS-based MCP server for interacting with the NestJS API",
    "main": "dist/main.js",
    "scripts": {
        "build": "nest build",
        "start": "node dist/main.js",
        "start:dev": "nest start --watch",
        "dev": "yarn build && node dist/main.js",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "api",
        "nestjs"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^0.6.0",
        "@nestjs/axios": "^3.0.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "axios": "^1.7.9",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@types/node": "^22.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "prettier": "^3.0.0",
        "typescript": "^5.0.0"
    }
}